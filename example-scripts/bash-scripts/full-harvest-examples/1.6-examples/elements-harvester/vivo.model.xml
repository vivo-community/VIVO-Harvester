<Model>
    <Param name="type">sdb</Param>   <!--  Vivo 1.2 uses rdb, Vivo 1.3 uses sdb -->
    <Param name="dbLayout">layout2</Param>
    <Param name="dbClass">com.mysql.jdbc.Driver</Param>
    <Param name="dbType">MySQL</Param>
    <Param name="dbUrl">jdbc:mysql://localhost/vivotest</Param>   <!--  the Jdbc connection url to your Vivo instance -->
    <Param name="dbUser">vivotest</Param>  <!--  database user -->
    <Param name="dbPass">vivotest</Param> <!--  database password -->
    <Param name="modelName">http://vitro.mannlib.cornell.edu/default/vitro-kb-2</Param>  <!--  this is the default model name of the live Vivo instance, this is the default -->
</Model>
