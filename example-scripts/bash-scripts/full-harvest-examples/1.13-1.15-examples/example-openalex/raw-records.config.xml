	<Param name="dbClass">com.mysql.jdbc.Driver</Param>
	<Param name="dbUrl">jdbc:mysql://127.0.0.1:3306/dbName</Param>
	<Param name="dbUser">myUser</Param>
	<Param name="dbPass">myPass</Param>
	<Param name="dbTable">myTableName</Param>
